clear;
load('../data/usseq.mat');
load('../results/usseqrects.mat');
for i=1:size(frames,3)-1
    i
    It = frames(:,:,i);
    It1 = frames(:,:,i+1);
    curr_rect = rects(i,:);
    curr_mask = SubtractDominantMotion(It(curr_rect(2):curr_rect(4),curr_rect(1):curr_rect(3))...
                                       ,It1(curr_rect(2):curr_rect(4),curr_rect(1):curr_rect(3)));
    curr_mask
    img_fused = imfuse(frames(:,:,i), curr_mask);
    imshow(img_fused);
    pause(0.01)
    if (i==30)|(i==60)|(i==90)|(i==120)
        saveas(gcf, strcat('aerial',int2str(i),'.png'));
    end
end
close